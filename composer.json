{
    "name": "axllent/silverstripe-weblog-wp-import",
    "description": "WordPress import tool for the Silverstripe Weblog module",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/axllent/silverstripe-weblog-wp-import",
    "keywords": [
        "silverstripe",
        "blog",
        "import"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ralph Slooten",
            "homepage": "http://www.axllent.org/"
        }
    ],
    "support": {
        "issues": "https://github.com/axllent/silverstripe-weblog-wp-import/issues"
    },
    "require": {
        "axllent/silverstripe-weblog": "^1.0",
        "axllent/simplehtmldom": "^1.5",
        "guzzlehttp/guzzle": "^7"
    },
    "extra": {
        "expose": [
            "css"
        ]
    },
    "suggest": {
        "axllent/silverstripe-weblog-categories": "optional: if you require categories"
    },
    "autoload": {
        "psr-4": {
            "Axllent\\WeblogWPImport\\": "src/"
        }
    }
}
